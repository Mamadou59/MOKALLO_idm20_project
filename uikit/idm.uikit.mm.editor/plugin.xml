<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated uikit -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="idm.uikit.mm.uikit.presentation.UikitModelWizardID"
            name="%_UI_UikitModelWizard_label"
            class="idm.uikit.mm.uikit.presentation.UikitModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/UikitModelFile.gif">
         <description>%_UI_UikitModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated uikit -->
      <editor
            id="idm.uikit.mm.uikit.presentation.UikitEditorID"
            name="%_UI_UikitEditor_label"
            icon="icons/full/obj16/UikitModelFile.gif"
            extensions="uikit"
            class="idm.uikit.mm.uikit.presentation.UikitEditor"
            contributorClass="idm.uikit.mm.uikit.presentation.UikitActionBarContributor">
      </editor>
   </extension>

</plugin>
