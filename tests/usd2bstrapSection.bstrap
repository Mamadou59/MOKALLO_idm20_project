<?xml version="1.0" encoding="UTF-8"?>
<bstrap:Page xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bstrap="http://www.example.org/bstrap" name="/tests/section.simplemarkdown">
  <pagecontents xsi:type="bstrap:Title" level="1">
    <textelements xsi:type="bstrap:Text" text="My First Bootstrap section 4 Page"/>
  </pagecontents>
  <pagecontents xsi:type="bstrap:FluidContainer">
    <generalProperties xsi:type="bstrap:Background" color="PRIMARY"/>
    <generalProperties xsi:type="bstrap:Margin" level="5" alignement="LEFT"/>
    <generalProperties xsi:type="bstrap:Padding" level="5"/>
    <generalProperties xsi:type="bstrap:Border"/>
    <generalProperties xsi:type="bstrap:ColoredBorder" color="SECONDARY"/>
    <pagecontents xsi:type="bstrap:Title" color="WARNING" level="1">
      <generalProperties xsi:type="bstrap:Background" color="PRIMARY"/>
      <generalProperties xsi:type="bstrap:Margin" level="5" alignement="LEFT"/>
      <generalProperties xsi:type="bstrap:Padding" level="5"/>
      <generalProperties xsi:type="bstrap:Border"/>
      <generalProperties xsi:type="bstrap:ColoredBorder" color="SECONDARY"/>
      <textelements xsi:type="bstrap:Text" text="Titre de la section"/>
    </pagecontents>
    <pagecontents xsi:type="bstrap:Paragraphe" color="WARNING">
      <generalProperties xsi:type="bstrap:Background" color="PRIMARY"/>
      <generalProperties xsi:type="bstrap:Margin" level="5" alignement="LEFT"/>
      <generalProperties xsi:type="bstrap:Padding" level="5"/>
      <generalProperties xsi:type="bstrap:Border"/>
      <generalProperties xsi:type="bstrap:ColoredBorder" color="SECONDARY"/>
      <textelements xsi:type="bstrap:Text" text="Ce paragraph est dans la prémière section"/>
    </pagecontents>
    <pagecontents xsi:type="bstrap:FixContainer">
      <generalProperties xsi:type="bstrap:Background" color="PRIMARY"/>
      <generalProperties xsi:type="bstrap:Margin" level="5" alignement="LEFT"/>
      <generalProperties xsi:type="bstrap:Padding" level="5"/>
      <generalProperties xsi:type="bstrap:Border"/>
      <generalProperties xsi:type="bstrap:ColoredBorder" color="SECONDARY"/>
      <pagecontents xsi:type="bstrap:Title" level="2">
        <textelements xsi:type="bstrap:Text" text="Titre de la sous section"/>
      </pagecontents>
      <pagecontents xsi:type="bstrap:UnOrderedList">
        <listitems>
          <listitemelements xsi:type="bstrap:Paragraphe">
            <textelements xsi:type="bstrap:Text" text="list in citation"/>
          </listitemelements>
        </listitems>
        <listitems>
          <listitemelements xsi:type="bstrap:Paragraphe">
            <textelements xsi:type="bstrap:Text" text="item 2"/>
          </listitemelements>
        </listitems>
        <listitems>
          <listitemelements xsi:type="bstrap:Paragraphe">
            <textelements xsi:type="bstrap:Text" text="item 3"/>
            <textelements xsi:type="bstrap:Text" text="fin de list"/>
          </listitemelements>
        </listitems>
      </pagecontents>
      <pagecontents xsi:type="bstrap:FixContainer">
        <generalProperties xsi:type="bstrap:Background" color="PRIMARY"/>
        <generalProperties xsi:type="bstrap:Margin" level="5" alignement="LEFT"/>
        <generalProperties xsi:type="bstrap:Padding" level="5"/>
        <generalProperties xsi:type="bstrap:Border"/>
        <generalProperties xsi:type="bstrap:ColoredBorder" color="SECONDARY"/>
        <pagecontents xsi:type="bstrap:Title" level="3">
          <textelements xsi:type="bstrap:Text" text="Titre de la sous sous section"/>
        </pagecontents>
        <pagecontents xsi:type="bstrap:Paragraphe" color="WARNING">
          <generalProperties xsi:type="bstrap:Background" color="PRIMARY"/>
          <generalProperties xsi:type="bstrap:Margin" level="5" alignement="LEFT"/>
          <generalProperties xsi:type="bstrap:Padding" level="5"/>
          <generalProperties xsi:type="bstrap:Border"/>
          <generalProperties xsi:type="bstrap:ColoredBorder" color="SECONDARY"/>
          <textelements xsi:type="bstrap:Text" text="text du paragraphe"/>
        </pagecontents>
      </pagecontents>
    </pagecontents>
  </pagecontents>
</bstrap:Page>
